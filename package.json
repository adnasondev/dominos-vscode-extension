{
  "name": "dominos-pizza-order",
  "displayName": "Dominos Pizza Order",
  "description": "An extension to order from Dominos Pizza.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dominos-pizza-order.placeOrder"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dominos-pizza-order.placeOrder",
        "title": "Place Dominos Order"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@vscode/test-electron": "^2.1.3",
    "glob": "^7.2.0"
  },
  "dependencies": {
    "dominos": "^3.3.1"
  }
}
